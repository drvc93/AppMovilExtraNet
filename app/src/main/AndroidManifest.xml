        <activity android:name=".LectorQRActivity" />
        <activity android:name=".ReadScanQR" />
        <activity
            android:name=".Login"
            android:label="LysApp">
            <intent-filter android:label="LysApp">
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegistrarUsuario" />
